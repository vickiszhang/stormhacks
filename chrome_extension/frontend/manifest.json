{
    "manifest_version": 3,
    "name": "Job Application Tracker",
    "version": "1.0",
    "description": "Track your job applications",
    "action": {
        "default_popup": "out/index.html"
    },
    "permissions": ["activeTab"],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    }
}